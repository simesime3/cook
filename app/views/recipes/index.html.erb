<div class="col-md-6 col-md-offset-3">
  <% @recipes.each do |f| %>
    <div class="text-center content-box">
      <h2 class="overflow"><%= f.title %></h2>
      <%= f.fields_for :picture do |p| %>
        <%= image_tag p.image.url, class: 'about-me-widget-200' %>
      <% end %>
    </div>
  <% end %>
</div>
